{
  "appId": "com.clubconnect.app",
  "appName": "Club Connect",
  "webDir": "frontend",
  "server": {
    "androidScheme": "https",
    "allowNavigation": [
      "club-connect-hvsz.onrender.com",
      "*.onrender.com",
      "*.supabase.com"
    ]
  },
  "plugins": {
    "StatusBar": {
      "overlaysWebView": false,
      "style": "DARK",
      "backgroundColor": "#17a38b"
    }
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true
  }
}